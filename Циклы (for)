#include <iostream>

int main() {
    int a, b;
    
    // Вводим целые числа a и b
    std::cin >> a >> b;
    
    // Проверяем и выводим четные числа
    for (int i = a; i <= b; i++) {
        if (i % 2 == 0) {
            std::cout << i << " ";
        }
    }
    
    std::cout << std::endl;
    
    return 0;
}
